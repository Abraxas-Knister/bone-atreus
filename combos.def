  │       │       │       │       │       │                        │       │       │       │       │       │
  ├───────┼─pgup──┼──up───┼─pgdw──┼───────┤                        ├───────┼───────┼───────┼───────┼───────┤
  │     home      │       │   •  end      │                        │       │   •︎   │       │       │       │
  ├─lmed──┼─left──┼─down──┼─right─┼───────┼───────┐        ┌───────┼───────┼───────┼───────┼───────┼───────┤
  │       │      tab   escape     │       │       │        │       │      del backspace  enter     │       │
  ├───────┼───── windows_one ─────┼───────┼───────┤        ├───────┼───────┼───── windows_two ─────┼───────┤
  │       │       │       │       │   •  alt      │        │     ctrl  •   │       │       │     steno     │
  └───────┴───────┴───────┴───────┴───────┴───────┘        └───────┴───────┴───────┴───────┴───────┴───────┘
*/
COMB(lmed,        OSL(lMED), KC_F,    KC_C)
COMB(pgup,        KC_PGUP,   KC_D,    KC_T)
COMB(pgdw,        KC_PGDN,   KC_A,    KC_E)
COMB(home,        KC_HOME,   KC_C,    KC_T)
COMB(end,         KC_END,    KC_E,    KC_O)
COMB(up,          KC_UP,     KC_U,    KC_I)
COMB(left,        KC_LEFT,   KC_T,    KC_V)
COMB(down,        KC_DOWN,   KC_I,    DE_UDIA)
COMB(right,       KC_RGHT,   KC_E,    DE_ADIA)
COMB(backspace,   KC_BSPC,   DE_Z,    KC_COMM)
COMB(del,         KC_DEL,    DE_Y,    DE_Z)
COMB(enter,       KC_ENT,    KC_COMM, KC_DOT)
COMB(escape,      KC_ESC,    DE_UDIA, DE_ADIA)
COMB(tab,         KC_TAB,    KC_V,    DE_UDIA)
COMB(alt,         _ALT_,     _SFT_,   QK_REP)
COMB(ctrl,        _CTL_,     QK_REP,  KC_SPC)
COMB(windows_one, _GUI_,     KC_V,    DE_UDIA, DE_ADIA)
COMB(windows_two, _GUI_,     DE_Z,    KC_COMM, KC_DOT)
TOGG(steno,       lSTENO,    K_STN_1, K_STN_2)

// symbol layer
/*
