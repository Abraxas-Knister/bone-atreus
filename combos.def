  │       │       │       │       │       │                        │       │       │       │       │       │
  ├───────┼───────┼───────┼───────┼───────┤                        ├───────┼───────┼───────┼───────┼───────┤
  │       │       │       │   •   │       │                        │       │   •   │       │       │       │
  ├──one──┼──two──┼─three─┼─four──┼─five──┼───────┐        ┌───────┼─six───┼─seven─┼─eight─┼─nine──┼─zero──┤
  │       │       │       │       │       │       │        │       │       │       │       │       │       │
  ├───────┼───────┼───────┼───────┼───────┼───────┤        ├───────┼───────┼───────┼───────┼───────┼───────┤
  │       │       │       │       │   •   │       │        │      gui  •   │       │       │       │       │
  └───────┴───────┴───────┴───────┴───────┴───────┘        └───────┴───────┴───────┴───────┴───────┴───────┘
*/
COMB(gui,  _GUI_, _ALT_,   _CTL_)
COMB(zero,  KC_0, DE_COLN, DE_SCLN)
COMB(one,   KC_1, DE_BSLS, DE_HASH)
COMB(two,   KC_2, DE_SLSH, DE_DLR)
COMB(three, KC_3, DE_LCBR, DE_PIPE)
COMB(four,  KC_4, DE_RCBR, DE_TILD)
COMB(five,  KC_5, DE_ASTR, DE_GRV)
COMB(six,   KC_6, DE_QUES, DE_PLUS)
COMB(seven, KC_7, DE_LPRN, DE_PERC)
COMB(eight, KC_8, DE_RPRN, DE_DQUO)
COMB(nine,  KC_9, DE_MINS, DE_QUOT)

